{
    "name": "mouf/javascript.ace",
    "description": "This package contains the ACE Javascript code editor (and a Mouf wrapper installer).",
    "keywords": ["javascript", "ace", "mouf"],
    "homepage": "https://mouf-php.com/packages/mouf/javascript.ace",
    "type": "mouf-library",
    "license": "MIT",
    "require": {
    	"php": ">=5.3.0",
    	"mouf/html.utils.weblibrarymanager": "~2.0"
    },
	"autoload" : {
		"psr-4" : {
			"Mouf\\Javascript\\Ace\\" : "lib/"
		}
	},
    "extra": {
	    "mouf": {
	    	"install": [{
	    		"type" : "class",
			"class" : "Mouf\\Javascript\\Ace\\AceInstaller"
	    	}]
	    }
	}
}
